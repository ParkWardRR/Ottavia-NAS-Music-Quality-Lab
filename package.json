{
  "name": "seville",
  "version": "1.0.0",
  "description": "Music Quality Lab - Build tools",
  "scripts": {
    "css:build": "npx tailwindcss -i ./web/static/css/src.css -o ./web/static/css/app.css --minify",
    "css:watch": "npx tailwindcss -i ./web/static/css/src.css -o ./web/static/css/app.css --watch",
    "build": "npm run css:build"
  },
  "devDependencies": {
    "tailwindcss": "^3.4.1",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.10"
  }
}
